<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="GulshanDev\LogRotate\Model\RotateConfig">
        <arguments>
            <argument name="dirName" xsi:type="const">Magento\Framework\App\Filesystem\DirectoryList::LOG</argument>
            <argument name="filenameFormat" xsi:type="string">*.log</argument>
        </arguments>
    </type>
</config>
